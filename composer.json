{
  "name": "payconn/wirecard",
  "description": "Wirecard payment processing library for PHP",
  "keywords": [
    "wirecard"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Murat SAC",
      "email": "muratsac@mail.com"
    }
  ],
  "type": "library",
  "require": {
    "php": ">=7.1.3",
    "guzzlehttp/guzzle": "^6.5",
    "symfony/http-foundation": "^4.4|^5.0",
    "payconn/common": "~1.3.8"
  },
  "autoload": {
    "psr-4": {
      "Payconn\\Wirecard\\": "src/Wirecard"
    },
    "classmap": [
      "src/Wirecard.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Payconn\\Wirecard\\Tests\\": "tests/Wirecard"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "phpstan/phpstan": "^0.11.2",
    "vimeo/psalm": "^3.9"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    }
  }
}
